<%- if controller_name != 'sessions' %>
  <p class="text-sm font-light text-gray-500 dark:text-gray-400">
    <%= link_to "Log in", new_session_path(resource_name), class: %w[font-medium text-primary-600 hover:underline dark:text-primary-500] %>
  </p>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <p class="text-sm font-light text-gray-500 dark:text-gray-400">
    <%= link_to "Sign up", new_registration_path(resource_name), class: %w[font-medium text-primary-600 hover:underline dark:text-primary-500] %>
  </p>
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <p class="text-sm font-light text-gray-500 dark:text-gray-400">
    <%= link_to "Forgot your password?", new_password_path(resource_name), class: %w[font-medium text-primary-600 hover:underline dark:text-primary-500] %>
  </p>
<% end %>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <p class="text-sm font-light text-gray-500 dark:text-gray-400">
    <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name), class: %w[font-medium text-primary-600 hover:underline dark:text-primary-500] %>
  </p>
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <p class="text-sm font-light text-gray-500 dark:text-gray-400">
    <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name), class: %w[font-medium text-primary-600 hover:underline dark:text-primary-500] %>
  </p>
<% end %>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <p class="text-sm font-light text-gray-500 dark:text-gray-400">
      <%= button_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), data: { turbo: false }, class: "btn-primary" %><br />
    </p>
  <% end %>
<% end %>
